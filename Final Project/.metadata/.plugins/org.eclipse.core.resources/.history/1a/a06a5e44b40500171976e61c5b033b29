                    npc = QuestNPC(quest_text = questOneText[0], accepted_text = questOneText[1], declined_text = questOneText[2], waiting_text = questOneText[3], turnin_text = questOneText[4], postquest_text = questOneText[5], quest_item='broach',reward=quest_reward)
